alt +  p
	rofi -show drun

super + c
	rofi -show calc -no-show-match -no-sort
alt + shift + w	
	 bash "$HOME/rofi/rofi-wifi-menu/rofi-wifi-menu.sh"
alt + shift + s
	bash "$HOME/.local/bin/powermenu.sh"
alt + shift + b
	bash "$HOME/.local/bin/bcn.sh"
super + w
	bash "$HOME/.local/bin/wallpaper.sh"
super + l
	betterlockscreen -l dim
super + q
	wallset -q

ctrl + super + l
	wallset --video Downloads/wagon.mp4
XF86AudioRaiseVolume
	bash $HOME/.local/bin/changevolume up

XF86AudioLowerVolume
	bash $HOME/.local/bin/changevolume down
XF86AudioMute
	bash $HOME/.local/bin/changevolume mute
ctrl + shift + d
	dunstctl close
ctrl + shift + h
	dunstctl history-pop
ctrl + shift + w
	bash "$HOME/.local/bin/wifi-checker.sh"
